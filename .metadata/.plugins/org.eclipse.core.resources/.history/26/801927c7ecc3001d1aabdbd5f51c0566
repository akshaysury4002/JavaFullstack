package com.as;

import java.util.*;

public class CommenUniqueInArray {
	
	public void commenEle(int[]a,int[]b)
	{
		 ArrayList<Integer> common = new ArrayList<Integer>();
		    for (int i = 0; i < a.length; i++) {
		        for (int j = 0; j < b.length; j++) {
		            if (a[i] == b[j]) {
		                common.add(a[i]);
		            }
		        }
		    }
		    System.out.println("Common elements: " + common);
		    
		    ArrayList<Integer> uniqueA = new ArrayList<Integer>();
		    for (int i = 0; i <a.length; i++) {
		        if (!common.contains(a[i])) {
		            uniqueA.add(a[i]);
		        }
		    }
		    System.out.println("Unique elements in first array: " + uniqueA);
		    
		    ArrayList<Integer> uniqueB = new ArrayList<Integer>();
		    for (int i = 0; i < b.length; i++) {
		        if (!common.contains(b[i])) {
		            uniqueB.add(b[i]);
		        }
		    }
		    System.out.println("Unique elements in second array: " + uniqueB);
	}

}
